<% @worksessions = @worksessions.sort_by{|a| a.begin_at.to_i} %>
<% @worksessions.each do |worksession| %>
  <% if worksession.begin_at >= @today && worksession.end_at < @today + 1.days  %>
    <%= worksession.date.strftime("%m/%d/%Y") %>, <%= worksession.begin_at.strftime("%I:%M %P") %>, <%= worksession.end_at.strftime("%I:%M %P")%>, <%= worksession.users.size > 0 %><br>
  <% end %>
<% end %>
